     ventas_detalles_27.`cod_pro` AS ventas_detalles_27_cod_pro,
     ventas_detalles_27.`des_pro` AS ventas_detalles_27_des_pro,
     ventas_detalles_27.`cant_pro` AS ventas_detalles_27_cant_pro,
