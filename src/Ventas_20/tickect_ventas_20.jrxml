     ventas_20.`fecha_ventas` AS ventas_20_fecha_ventas,
     ventas_20.`fecha_control_ventas` AS ventas_20_fecha_control_ventas,
     ventas_20.`user_ventas` AS ventas_20_user_ventas,
     ventas_20.`almacen_ventas` AS ventas_20_almacen_ventas,
     ventas_20.`cant_ventas` AS ventas_20_cant_ventas,
     ventas_20.`letras_ventas` AS ventas_20_letras_ventas,
     ventas_20.`nro_fact_ventas` AS ventas_20_nro_fact_ventas,
     ventas_20.`vac_ventas` AS ventas_20_vac_ventas,
     ventas_detalles_20.`num_bol` AS ventas_detalles_20_num_bol,
     ventas_detalles_20.`cod_pro` AS ventas_detalles_20_cod_pro,
     ventas_detalles_20.`des_pro` AS ventas_detalles_20_des_pro,
     ventas_detalles_20.`cant_pro` AS ventas_detalles_20_cant_pro,
     ventas_detalles_20.`pre_unit` AS ventas_detalles_20_pre_unit,
     ventas_detalles_20.`cant_kg` AS ventas_detalles_20_cant_kg,
     ventas_detalles_20.`pre_venta` AS ventas_detalles_20_pre_venta,
     ventas_detalles_20.`data` AS ventas_detalles_20_data,
     ventas_detalles_20.`id_pre_det` AS ventas_detalles_20_id_pre_det
FROM
     `ventas_20` ventas_20 INNER JOIN `ventas_detalles_20` ventas_detalles_20 ON ventas_20.`num_bol` = ventas_detalles_20.`num_bol`,
     `empresas` empresas
