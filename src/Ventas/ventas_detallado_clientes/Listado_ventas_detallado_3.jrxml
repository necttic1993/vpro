	<queryString>
		<![CDATA[SELECT
     empresas.`id_empre` AS empresas_id_empre,
     empresas.`emp_nom` AS empresas_emp_nom,
     empresas.`emp_des` AS empresas_emp_des,
     empresas.`emp_imag` AS empresas_emp_imag,
