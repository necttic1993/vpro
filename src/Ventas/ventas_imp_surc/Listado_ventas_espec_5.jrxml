FROM
     `empresas` empresas,
     `ventas_5` ventas_5
WHERE
     ventas_5.`fecha_ventas` BETWEEN $P{fecha_ini} AND $P{fecha_fin}
 AND ventas_5.`estado_ventas` = $P{prove}]]>
